<h1>Sign In</h1>


<%= form_with url: sign_in_path, local: true, data: { turbo: false }  do |form| %>
	<div class="mb-3">
		<%= form.label :email %>
		<%= form.text_field :email, class: "form-controler" %>
	</div>
	<div class = "mb-3">
		<%= form.label :password %>
		<%= form.password_field :password_digest, class: "form-controler" %>
	</div>
	<div class="mb-3">
		<%= form.submit "Sign In", class: "btn btn-primary" %>
	</div>
<% end %>